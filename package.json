{
  "name": "inventory_manager_express",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "nodemon -r tsconfig-paths/register src/index.ts",
    "db:generate": "drizzle-kit generate --config=./drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=./drizzle.config.ts",
    "db:submit": "drizzle-kit generate --config=./drizzle.config.ts && drizzle-kit migrate --config=./drizzle.config.ts",
    "db:push": "drizzle-kit push --config=./drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config=./drizzle.config.ts",
    "db:seed": "tsx src/db/seed.ts",
    "db:create": "drizzle-kit push --config=./drizzle.config.ts && tsx src/db/seed.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.35.3",
    "express": "^4.21.1",
    "express-validator": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.11.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.8.1",
    "drizzle-kit": "^0.26.2",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}